generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Channel {
  id String @id @default(uuid())
  name String
  ccreatedAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  workspaceId String
  createdById String

  @@unique([workspaceId, name])
}